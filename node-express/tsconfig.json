{
  "compilerOptions": {
    "outDir": "./dist/",
    "lib": ["ESNext"],
    "module": "CommonJS",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "allowJs": true,
    "types": ["node"]
  }
}
